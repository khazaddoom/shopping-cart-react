 found.qty++\r\n      cartData[cartData.indexOf(found)] = found\r\n      setcartData([...cartData])\r\n    } else {\r\n      setcartData([...cartData, { ...data.find(item=>item.id == id), qty: 1 }])\r\n    }\r\n  }\r\n\r\n  const handleRemoveFromCart = (id) => {\r\n    let found = cartData.find(prod => prod.id == id)\r\n    if (found) {\r\n      if (found.qty == 1) {\r\n        setcartData([...cartData.slice(0, cartData.indexOf(found)), ...cartData.slice(cartData.indexOf(found) + 1)])\r\n      }\r\n      else {\r\n        found.qty--\r\n        cartData[cartData.indexOf(found)] = found\r\n        setcartData([...cartData])\r\n      }\r\n    } else {\r\n        setcartData([])\r\n      }\r\n  }\r\n\r\n  return (\r\n    <Router>\r\n      <Switch>\r\n        <Route exact path=\"/\">\r\n          <Redirect to=\"/products\" />\r\n        </Route>\r\n        <Route path=\"/products\">\r\n          <WithHeader cartQuantity={cartData.length}>\r\n            <ProductList products={data} cartItems={cartData} handleAddToCart={handleAddToCart} handleRemoveFromCart={handleRemoveFromCart} />\r\n          </WithHeader>\r\n        </Route>\r\n        <Route path=\"/cart\">\r\n          <WithHeader cartQuantity={cartData.length}>\r\n            <Cart cartItems={cartData} handleAddToCart={handleAddToCart} handleRemoveFromCart={handleRemoveFromCart} data={data}/>\r\n          </WithHeader>\r\n        </Route>\r\n        <Route>\r\n          <WithHeader cartQuantity={cartData.length}>\r\n            <NoMatch />\r\n          </WithHeader>\r\n        </Route>\r\n      </Switch>\r\n    </Router>\r\n  );\r\n}\r\n\r\nexport default App;\r\n","C:\\Users\\Ganesh\\Desktop\\Learning\\shopping-cart-react\\src\\components\\ProductList.js",["48"],"import React from 'react'\r\nimport Product from './Product'\r\nimport './ProductList.css'\r\n\r\nlet id = 1\r\n\r\nconst ProductList = ({products, handleAddToCart, cartItems, handleRemoveFromCart}) => {\r\n\r\n    return (\r\n        <div className=\"grid\">\r\n            {\r\n                products.map(product => {\r\n                    let found = cartItems.find(item => item.id == product.id)\r\n                    product.qty = (found && found.qty) || 0\r\n                    return product\r\n                })\r\n                .map(product => (<Product key={id++} {...product} handleAddToCart={handleAddToCart} handleRemoveFromCart={handleRemoveFromCart}/>))\r\n            }\r\n            \r\n        </div>\r\n    )\r\n}\r\n\r\nexport default ProductList\r\n","C:\\Users\\Ganesh\\Desktop\\Learning\\shopping-cart-react\\src\\components\\WithHeader.js",[],"C:\\Users\\Ganesh\\Desktop\\Learning\\shopping-cart-react\\src\\components\\NoMatch.js",[],"C:\\Users\\Ganesh\\Desktop\\Learning\\shopping-cart-react\\src\\components\\Cart.js",[],"C:\\Users\\Ganesh\\Desktop\\Learning\\shopping-cart-react\\src\\components\\Product.js",["49"],"import React, {useState} from 'react'\r\nimport './Product.css'\r\n\r\nconst Product = ({id, title, shortDescription, imageUrl, unitPrice, currencyCode, qty, handleAddToCart, handleRemoveFromCart}) => {\r\n\r\n    const [productQty, setproductQty] = useState(qty)\r\n\r\n    const increaseProductQty = e => {\r\n        e.preventDefault()\r\n        setproductQty(productQty+1)\r\n        handleAddToCart(id)\r\n    }\r\n\r\n    const decreaseProductQty = e => {\r\n        e.preventDefault()\r\n        setproductQty(productQty-1)\r\n        handleRemoveFromCart(id)\r\n    }\r\n\r\n    return (\r\n        <div className=\"card-producto\">\r\n            <h1>{title}</h1>\r\n            <p>{shortDescription}</p>\r\n            <div className=\"pic-producto\">\r\n                <span className=\"green\" data-color=\"#55efc4\" data-pic=\"url(3.png)\" />\r\n            </div>\r\n            <div className=\"info-producto\">\r\n                <div className=\"price-producto\">{currencyCode + unitPrice}</div>\r\n                {productQty>0? <div><button onClick={decreaseProductQty}> - </butt